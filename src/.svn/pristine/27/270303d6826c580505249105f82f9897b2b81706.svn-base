* {
  font-size: 14px;
}
body {
  overflow: hidden;
  background: #f0f2f5;
}

::-webkit-scrollbar-track-piece{width:10px;height: 10px;background-color:#f0f0f0 ;  }

::-webkit-scrollbar{width:10px;height:10px ; }

::-webkit-scrollbar-thumb{height:50px;width: 50px;background:#cdcdcd;   cursor:pointer;}

::-webkit-scrollbar-thumb:hover{background:#cdcdcd ; cursor:pointer}

.ant-tooltip {
  max-width: 400px!important;
}